% \setlength{\fboxrule}{0.5mm}
\newcommand{\cfbox}[2]{%
    \color{green}%
    \addtolength{\linewidth}{-2\fboxsep}%
    \addtolength{\linewidth}{-2\fboxrule}%

    \fbox{%
      \color{black}%
        \begin{minipage}{#1 + \linewidth}
	  #2
	\end{minipage}
    }
    \color{black}
}

